@model IEnumerable<ProyectoControlLineaBus.Models.MuestraReport>

@{
    ViewBag.Title = "Reporte Fechas Empleado";
    Layout = "_Layout.cshtml";
}

<h2 class="text-center">Reporte Fechas @ViewBag.Start - @ViewBag.End</h2>
<h3 class="text-center"> NIT: @ViewBag.NIT</h3>
<table class="table text-center">
    <tr>

        <th style="text-align:center">
            Nombre
        </th>
        <th style="text-align:center">
            Placa
        </th>
        <th style="text-align:center">
            Punto de Control
        </th>
        <th style="text-align:center">
            Fecha de Registro
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.plate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.descCheckpoint)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dateRegister)
            </td>
        </tr>
    }

</table>
Fecha de creación de reporte: @ViewBag.Hoy
