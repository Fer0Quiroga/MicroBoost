@model IEnumerable<ProyectoControlLineaBus.Models.MuestraReport>

@{
    ViewBag.Title = "Reporte Fechas";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="~/assets/css/main.css" />
    <noscript><link rel="stylesheet" href="~/assets/css/noscript.css" /></noscript>

</head>
<div style="margin-top:5%"></div>

<h2 class="text-center">Reporte Fechas @ViewBag.Start - @ViewBag.End</h2>
<div>
    @Html.ActionLink("Imprimir", "PrintReportDates", "", new { @class = "btn btn-info mb-2" })
</div>
<table class="table text-center text-white">
    <tr>

        <th style="text-align:center">
            Nombre
        </th>
        <th style="text-align:center">
            NIT
        </th>
        <th style="text-align:center">
            Placa
        </th>
        <th style="text-align:center">
            Punto de Control
        </th>
        <th style="text-align:center">
            Fecha de Registro
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idEmployee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.plate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.descCheckpoint)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dateRegister)
            </td>
        </tr>
    }

</table>
Fecha de creación de reporte: @ViewBag.Hoy
